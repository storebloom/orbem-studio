{
  "name": "orbem-studio",
  "version": "1.0.3",
  "description": "Dependencies for Orbem Studio plugin",
  "devDependencies": {
    "@wordpress/scripts": "^30.15.0",
    "dotenv": "^16.5.0",
    "prettier": "^3.5.3",
    "regenerator-transform": "^0.15.2"
  },
  "scripts": {
    "build": "wp-scripts build assets/src/js/*.js --output-path=assets/build/",
    "composer": "wp-env run cli --env-cwd=wp-content/plugins/orbem-studio composer",
    "env:build": "wp-env build",
    "env:destroy": "wp-env destroy",
    "env:shell": "wp-env run cli bash",
    "env:start": "wp-env start",
    "env:stop": "wp-env stop",
    "format-js": "wp-scripts lint-js assets/src/js/ --fix",
    "format-php": "npm run composer fix",
    "format": "npm run format-js && npm run format-php",
    "lint-js": "wp-scripts lint-js assets/src/js/",
    "lint-php": "npm run composer lint",
    "logs": "wp-env logs",
    "setup": "npm ci && composer install",
    "watch:hot": "wp-scripts start assets/src/js/*.js --output-path=assets/build/ --hot",
    "watch": "wp-scripts start assets/src/js/*.js --output-path=assets/build/",
    "wp": "wp-env run cli wp",
    "zip": "zip -r ../$npm_package_name.zip * -x@.distignore"
  },
  "engines": {
    "node": ">=22.4.0",
    "npm": ">=10.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/storebloom/orbem-studio.git"
  },
  "keywords": [
    "orbemorder.com"
  ],
  "author": "Scott Adrian",
  "contributors": [
    {
      "name": "Scott Adrian",
      "email": "scott.adrian@orbemorder.com"
    },
    {
      "name": "Scott Weaver",
      "email": "scottmweaver@gmail.com"
    }
  ],
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/storebloom/orbem-studio/issues"
  },
  "homepage": "https://github.com/storebloom/orbem-studio#readme",
  "prettier": {
    "bracketSpacing": true,
    "jsxSingleQuote": true,
    "singleQuote": true,
    "tabWidth": 4
  },
  "dependencies": {
    "@wordpress/env": "^10.37.0"
  }
}
